+        # Locate list of subnets to which this subnet has to be appended
+        net_ipam_ref = None
+        ipam_refs = net_obj.get_network_ipam_refs()
+        if ipam_refs:
+            for ipam_ref in ipam_refs:
+                if ipam_ref['to'] == ipam_fq_name:
